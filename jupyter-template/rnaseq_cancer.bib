  urldate = {2018-01-25},
  url = {http://www.sciencedirect.com/science/article/pii/S0302283813001346},
  journal = {European Urology},
  author = {Muselaers, Constantijn H. J. and Boerman, Otto C. and Oosterwijk, Egbert and Langenhuijsen, Johannes F. and Oyen, Wim J. G. and Mulders, Peter F. A.},
  month = jun,
