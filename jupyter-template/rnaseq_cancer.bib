  journal = {BMC Medical Genomics},
  author = {Sims, Andrew H. and Smethurst, Graeme J. and Hey, Yvonne and Okoniewski, Michal J. and Pepper, Stuart D. and Howell, Anthony and Miller, Crispin J. and Clarke, Robert B.},
  month = sep,
