  file = {/Users/Jvivian/Zotero/storage/4XBHXF7K/Newbold et al. - 2009 - An Exploratory Study Into the Role of Dynamic Cont.pdf;/Users/Jvivian/Zotero/storage/J497JL4S/S0360301608031799.html}
