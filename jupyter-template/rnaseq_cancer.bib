  author = {Kirk, Allan D. and Hale, Douglas A. and Mannon, Roslyn B. and Kleiner, David E. and Hoffmann, Steven C. and Kampen, Robert L. and Cendales, Linda K. and Tadaki, Douglas K. and Harlan, David M. and Swanson, S. John},
  month = jul,
