  journal = {The Lancet Oncology},
  author = {Choueiri, Toni K and Escudier, Bernard and Powles, Thomas and Tannir, Nizar M and Mainwaring, Paul N and Rini, Brian I and Hammers, Hans J and Donskov, Frede and Roth, Bruce J and Peltola, Katriina and Lee, Jae Lyun and Heng, Daniel Y C and Schmidinger, Manuela and Agarwal, Neeraj and Sternberg, Cora N and McDermott, David F and Aftab, Dana T and Hessel, Colin and Scheffold, Christian and Schwab, Gisela and Hutson, Thomas E and Pal, Sumanta and Motzer, Robert J},
