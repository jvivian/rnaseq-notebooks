  journal = {The Lancet},
  author = {Fehrenbacher, Louis and Spira, Alexander and Ballinger, Marcus and Kowanetz, Marcin and Vansteenkiste, Johan and Mazieres, Julien and Park, Keunchil and Smith, David and Artal-Cortes, Angel and Lewanski, Conrad and Braiteh, Fadi and Waterkamp, Daniel and He, Pei and Zou, Wei and Chen, Daniel S and Yi, Jing and Sandler, Alan and Rittmeyer, Achim},
