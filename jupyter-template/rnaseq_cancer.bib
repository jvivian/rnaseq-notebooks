  journal = {Clinical Cancer Research},
  author = {Nomi, Takeo and Sho, Masayuki and Akahori, Takahiro and Hamada, Kaoru and Kubo, Atsushi and Kanehiro, Hiromichi and Nakamura, Shinji and Enomoto, Koji and Yagita, Hideo and Azuma, Miyuki and Nakajima, Yoshiyuki},
  month = apr,
